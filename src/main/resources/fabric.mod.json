{
  "schemaVersion": 1,
  "id": "opencomputers",
  "version": "${version}",
  "name": "OpenComputers",
  "description": "OpenComputers adds programable computers to Minecraft.",
  "authors": [
    "lucsoft"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/lucsoft/OpenComputers",
    "issues": "https://github.com/lucsoft/OpenComputers/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/opencomputers/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "li.cli.oc.OpenComputers"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "li.cli.oc.client.ClientInit"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "depends": {
    "fabric-language-kotlin": ">=1.4.0"
  },
  "suggests": {
  }
}
